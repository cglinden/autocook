net-snmp-5.2.1_version = 5.2.1;
net-snmp-5.2.1_dir = [thirdparty]/net-snmp-[net-snmp-5.2.1_version];
net-snmp-5.2.1_use_unpack  = [net-snmp-5.2.1_dir]/[dummy];
net-snmp-5.2.1_cpp_I_flags = ADD [__FILE__]':'[__LINE__] -I[net-snmp-5.2.1_dir]/include;
net-snmp-5.2.1_cc_I_flags  = ADD [__FILE__]':'[__LINE__] -I[net-snmp-5.2.1_dir]/include;
net-snmp-5.2.1_cc_I_flags += ADD [__FILE__]':'[__LINE__] -I[net-snmp-5.2.1_dir]/include/ucd-snmp;
net-snmp-5.2.1_ld_l_flags  = ADD [__FILE__]':'[__LINE__] -ldl -lcrypto -lm; 
net-snmp-5.2.1_external_obj = ADD [__FILE__]':'[__LINE__]
  "'-Wl,-('"
  [net-snmp-5.2.1_dir]/lib/libnetsnmp.a
  [net-snmp-5.2.1_dir]/lib/libnetsnmpmibs.a
  [net-snmp-5.2.1_dir]/lib/libnetsnmpagent.a
  [net-snmp-5.2.1_dir]/lib/libnetsnmphelpers.a
  [net-snmp-5.2.1_dir]/lib/libnetsnmptrapd.a
  "'-Wl,-)'"
;

[combine [net-snmp-5.2.1_external_obj]]: [net-snmp-5.2.1_use_unpack];

net-snmp-5.2.1_cc_D_flags_optimized  = ADD [__FILE__]':'[__LINE__] -DHAVE_STRDUP;
net-snmp-5.2.1_cpp_D_flags_optimized = ADD [__FILE__]':'[__LINE__] -DHAVE_STRDUP;
net-snmp-5.2.1_use_dependency = ADD [__FILE__]':'[__LINE__] [__FILE__];
